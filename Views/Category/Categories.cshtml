@model ViewModels
<div class="header">
    <a href="/">Products</a> | Categories
</div>

<div class="form-div">
    <partial name="_createCategories.cshtml" model="@Model.CategoryModel"/>
</div>

<div class="item-list">
    <div class="block-title">
        Existing Categories
    </div>
    @foreach(var category in Model.Categories)
    {
        <div class="list-group">
            <a href="GetCategoryDetail/@category.CategoryId" class="list-group-item list-group-item-action">
                @category.name
            </a>
        </div>
    }
</div>